{
  "private": true,
  "name": "vulmix-app",
  "scripts": {
    "dev": "tsc ./vulmix.config.ts --outDir ./.vulmix && mix watch --mix-config=.vulmix/laravel-mix/webpack.mix.js --hot",
    "prod": "tsc ./vulmix.config.ts --outDir ./.vulmix && mix --mix-config=.vulmix/laravel-mix/webpack.mix.js --production",
    "serve": "npm run prod && npx http-server -p 8000 -a localhost _dist --gzip --proxy http://localhost:8000?"
  },
  "dependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "vulmix": "0.6.0-beta"
  }
}
